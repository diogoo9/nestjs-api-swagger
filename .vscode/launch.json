  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run test",
      "name": "npm run test",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npm run test producer.controller.spec.ts",
      "name": "npm run test producer.controller.spec.ts",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "npm run dev",
      "name": "Run npm dev",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    }
  ]
}